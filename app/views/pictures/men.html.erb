<div class="container">
  <div class="indexHeadBox">
    <table class="table">
      <tr>
        <td class="indexWomenBox"><a href="/gender/women">Women</a></td>
      </tr>
    </table>
  </div>
  <div class="indexMainBox col-md-12">
    <div class="indexBox1">
      <h4><i class="glyphicon glyphicon-camera"></i> 新着コーディネート( 男性 )</h4>
    </div>
    <hr>
    <div class="indexBox2 clearfix">
      <div class="row">
        <ul class="pictures">
          <% @men.each do |men| %>
            <li>
              <a href="/pictures/<%= men.id %>"><%= image_tag("#{men.image}", height: "200px", width: "150px") %></a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
    <div class="pagiNation row">
      <div class="page col-md-12">
        <%= paginate(@men) %>
      </div>
    </div>
  </div>
</div>
